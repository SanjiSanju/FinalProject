			<option value="9">9</option>
			<option value="10">10</option>
			<option value="Intermediate">Intermediate</option>
			<option value="others">Single Courses</option>
		  </select>
		</div>
	  
	  
		<div class="form-group">
		  <label for="exampleInputRequirements">Requirements</label>
		  <textarea class="form-control rounded-0" id="requirements" name="requirements" rows="6" placeholder="Enter Your Other Requirements"></textarea>
		</div>
		
		<div class="form-group">
			<label for="exampleInputTutionFee">Affordable Tution Fee</label>
			<input type="text" class="form-control" id="tutionfee" name="tutionfee" placeholder="Enter Maximum Affordable Tution Fee">
		  </div>
		<button type="submit" class="btn btn-primary">Submit</button>
